{
  "name": "DynaFav2",
  "version": "2.0.8",
  "manifest_version": 2,
  "description": "Dynically modifies salesforce favicons",
  "icons": { "128": "icons/dynafav128.png"},
  "options_page": "options.html",
  "background": {
    "page": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.salesforce.com/*"],
      "js": ["includes/dynafav2.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
